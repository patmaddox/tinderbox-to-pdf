<attribute name="HTMLExportCommand" >PATH=/Library/TeX/texbin:$PATH /usr/local/bin/pandoc -s -r html -V colorlinks=true -o ~/Documents/tinderbox-to-pdf-v0_1_1.pdf -V documentclass=report --toc</attribute>
<attribute name="IsComposite" >false</attribute>
<attribute name="KeyAttributes" >PDFAuthor;PDFDate;PDFExportFileName;PDFIncludeTOC;PDFDocumentClass</attribute>
<attribute name="Modified" >2017-12-09T21:54:44-07:00</attribute>
<attribute name="Name" >Create Beautiful PDFs From Your Tinderbox Documents</attribute>
<attribute name="PDFAuthor" >Pat Maddox – pat@patmaddox.com</attribute>
<attribute name="PDFDate" >v0.1.0 – December 9th, 2017</attribute>
<attribute name="PDFDocumentClass" >report</attribute>
<attribute name="PDFExportFileName" >~/Documents/tinderbox-to-pdf-v0_1_1.pdf</attribute>
<attribute name="PDFIncludeTOC" >true</attribute>
<attribute name="Rule" >var whichLatex;
var whichPandoc;

whichLatex = runCommand(&quot;which /Library/TeX/texbin/xelatex&quot;);
whichPandoc = runCommand(&quot;which /usr/local/bin/pandoc&quot;);

$Badge = &quot;x&quot;;
$HTMLExportCommand = ;

if(whichLatex.contains(&quot;/Library/TeX/texbin/xelatex&quot;) &amp; whichPandoc.contains(&quot;/usr/local/bin/pandoc&quot;)) {
  $HoverExpression = ;
  if(!$PDFExportFileName) { $PDFExportFileName = &quot;~/Documents/tinderbox-to-pdf.pdf&quot;; };

  var CommandString;

  CommandString=&quot;PATH=/Library/TeX/texbin:$PATH /usr/local/bin/pandoc -s -r html -V colorlinks=true -o &quot; + $PDFExportFileName;

  if($PDFDocumentClass &amp; !CommandString.contains(&quot;-V documentclass=&quot; + $PDFDocumentClass)) { CommandString = CommandString + &quot; -V documentclass=&quot; + $PDFDocumentClass; };

  if($PDFIncludeTOC &amp; !CommandString.contains(&quot;--toc&quot;)) { CommandString = CommandString + &quot; --toc&quot;; };

  $HTMLExportCommand = CommandString;
  $Badge = &quot;check mark&quot;;
} else {
  var hoverExpression(&quot;Missing&quot;);

  if(!whichLatex.contains(&quot;/Library/TeX/texbin/xelatex&quot;)) {
    hoverExpression = hoverExpression + &quot; latex&quot;;
  };

  if(!whichPandoc.contains(&quot;/usr/local/bin/pandoc&quot;)) {
    hoverExpression = hoverExpression + &quot; pandoc&quot;;
  };

  $HoverExpression = hoverExpression;
};</attribute>
